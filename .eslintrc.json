{
	"env": {
		"node": true,
		"browser": true,
		"es2021": true
	},
	"extends": [
		// "eslint:recommended",
		"plugin:react/recommended",
		"plugin:react/jsx-runtime",
		"prettier"
	],
	"overrides": [],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["react", "@typescript-eslint"],
	"settings": {
		"react": {
			"version": "detect"
		}
	}
}
